.form-inline {
	width: 150px;
}
.form-inline-sec {
	width: 150px;
}
.form-inline-thr {
	width: 150px;
}
.active {
	padding-left: 10px;   /* 좌측 여백 설정 */
  	padding-right: 10px;  /* 우측 여백 설정 */
}
th{
	width: 200px;
}
table, th, td, tr {
            border: 1px solid #424242; /* 테두리 속성 설정 */
            background-color: #FAFAFA;
            
}
.table table-bordered tbl_Form{
	border-radius: 10px;
}

select, input{
	text-align: center;
}

.form-control{
	border-color: #424242;
}
.page-header{
	font-size: 2.5em;
}
.clsHr{
	background:black;
    height:1px;
    border:0;
}
.clsHr{
	border: 1px solid black;
}
.col-lg-12{
	border: 0px
}
</style>

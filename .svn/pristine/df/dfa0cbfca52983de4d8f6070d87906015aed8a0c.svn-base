<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.mapper.ObjectionMapper">
	
	
	
	<insert id="insertObjection" parameterType="objectionVO">
		<selectKey resultType="String" keyProperty="objCd" order="BEFORE">
			SELECT OBJECTION_SEQ.NEXTVAL FROM DUAL
		</selectKey>
		INSERT INTO OBJECTION(OBJ_CD, CRTR_CD, STU_CD, APP_CD, OBJ_CON)
		VALUES (#{objCd}, #{crtrCd}, #{stuCd}, #{appCd}, #{objCon})
	</insert>
</mapper>
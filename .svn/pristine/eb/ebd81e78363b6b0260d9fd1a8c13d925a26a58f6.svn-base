<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%>
<%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags" %>
<script src="/resources/js/jquery.min.js"></script>

<style>
	.reservation_box{width:100%;}
	.reservation_box .table_topic{font-size: 20px; padding-bottom:30px;}
	.reservation_box .table_topic i{margin-left:10px;}
	.reservation_box table{font-size:14px; width:100%; text-align:center;}
	.table_title{font-size:13px; border-top:1px solid #d1d0d0; border-bottom:1px solid #d1d0d0; height:32px; line-height:32px;}
	.reservation_box tr{}
	.reservation_box tr td:nth-child(1){width:5%;}
	.reservation_box tr td:nth-child(2){width:10%;}
	.reservation_box tr td:nth-child(3){width:20%;}
	.reservation_box tr td:nth-child(4){width:10%;}
	.reservation_box tr td:nth-child(5){width:15%;}
	.reservation_box tr td:nth-child(6){width:15%;}
	.reservation_box tr td:nth-child(7){width:25%;}
</style>
<div class="reservation_box">
	<div class="table_topic">예약내역<i class="fa-solid fa-circle-info" style="color: #333333;"></i></div>
	<table>
		<tr class="table_title">
			<td>순번</td>
			<td>건물</td>
			<td>호실</td>
			<td>예약일</td>
			<td>시작시간</td>
			<td>종료시간</td>
			<td>비고</td>
		</tr>
		<c:forEach items="${reservationList}" varStatus="cnt" var="resVO">
			<tr>
				<td>${cnt.count}</td>
				<td>${resVO.commonDetailVO.comdNm}</td>
				<td>${fn:substring(resVO.roomCd, 6, 9)}</td>
				<td><fmt:formatDate value="${resVO.resStdt}" pattern="yy-MM-dd"/></td>
				<td><fmt:formatDate value="${resVO.resStdt}" pattern="HH:mm"/></td>
				<td><fmt:formatDate value="${resVO.resEddt}" pattern="HH:mm"/></td>
				<td></td>
				<td></td>
			</tr>
		</c:forEach>
	</table>
</div>












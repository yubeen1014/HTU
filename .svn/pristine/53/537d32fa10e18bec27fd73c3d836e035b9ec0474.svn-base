<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags" %>
<section class="content container">
<sec:authentication property="principal.userVO" var="userVO"/>
<style>
  .frmDiv {
    border: 1px solid red; 
  }
    .frmDiv input {
    border: 1px solid black;
  }
</style>
	<div class="frmDiv">
<%-- 		<p>${userVO}</p> --%>
		<form action="/pro/createlectureapplyconsent?${_csrf.parameterName}=${_csrf.token}"
			name="frm" method="post">
			<div>
				<p><label><input type="hidden" id="userCd" name="consentVO.userCd" value="${userVO.userCd}" />${userVO.userCd}</label></p>
				<p><label><input type="hidden" id="comdCd" name="consentVO.comdCd" value="CON4" />강의계획신청</label></p>
				<p><label><input type="hidden" id="conTarget" name="consentVO.conTarget" value="1001" />관리자</label></p>
			</div>	
			<p>${consentVO}</p>
<%-- 			${commonDetailList} --%>
			<div>
<%-- 				<p>교번<input type="hidden" id="proCd"  name="proCd" value="${userVO.userCd}" />${userVO.userCd}</p>	 --%>
<%-- 				<p>승인번호<input type="hidden" id="conCd"  name="lectureApplyVO.conCd" value="${consentVO.conCd}" /></p> --%>

				<p><select
					name="roomCd">
<%-- 					<c:forEach var="CommonDetailVO" items="${commonDetailList}"> --%>
<%-- 						<c:if test="${CommonDetailVO.comCd=='BLD'}"> --%>
							<c:forEach var="roomVO" items="${roomVOList}">
							<option value="${roomVO.roomCd}"
								>${roomVO.roomCd}</option>
							</c:forEach>
<%-- 						</c:if> --%>
<%-- 					</c:forEach> --%>
				</select></p>
								<p><select
					name="depCd">
<%-- 					<c:forEach var="CommonDetailVO" items="${commonDetailList}"> --%>
<%-- 						<c:if test="${CommonDetailVO.comCd=='COL'}"> --%>
							<c:forEach var="departmentVO" items="${departmentVOList}">
							<option value="${departmentVO.depCd}"
								>${departmentVO.depCd}</option>
							</c:forEach>
<%-- 						</c:if> --%>
<%-- 					</c:forEach> --%>
				</select></p>
				<p>년도<input type="number" id="lecaYear"  name="lecaYear" value="" /></p>	
				<p><label>학기</label><select name="lecaSem">
					<option value="1" selected="selected">1</option>
					<option value="2">2</option>
				</select></p>
				<p><label>강의명<input type="text" id="lecaNm"  name="lecaNm" value="" /></label></p>	
				<p><label>교과목개요<input type="text" id="lecaCon"  name="lecaCon" value="" /></label></p>
				<p><label>개설학년</label><select name="lecaTrg">
					<option value="1" selected="selected">1</option>
					<option value="2">2</option>
					<option value="3">3</option>
					<option value="4">4</option>
				</select></p>	
				<p><label>학점</label><select name="lecaCrd">
					<option value="1" selected="selected">1</option>
					<option value="2">2</option>
					<option value="3">3</option>
					<option value="4">4</option>
					<option value="4">5</option>
					<option value="4">6</option>
				</select></p>	
				<p><label>강의시수<input type="number" id="lecaPer"  name="lecaPer" value="" /></label></p>	
				<p><label>정원<input type="number" id="lecaCap"  name="lecaCap" value="" /></label></p>	
				<p><label>희망일정(요일)<input type="text" id="lecaSche"  name="lecaSche" placeholder="'월요일, 수요일' 의 형식으로 입력하시오" /></label></p>	
				<p><label>강의시작시간<input type="text" id="lecaStdt"  name="lecaStdt" value="" placeholder="'12:00' 의 형식으로 입력하시오" /></label></p>	
				<p><label>강의종료시간<input type="text" id="lecaEddt"  name="lecaEddt" value="" placeholder="'12:00' 의 형식으로 입력하시오" /></label></p>	
				<p><label>교재 및 참고서<input type="text" id="lecaBook"  name="lecaBook" value="" /></label></p>	
				<p><label>비고<input type="text" id="lecaNote"  name="lecaNote" value="" /></label></p>
				<p><label>평가구분</label><select name="lecaEva">
					<option value="0" selected="selected">상대평가</option>
					<option value="1">절대평가</option>
				</select></p>	
				<p><label>저장선택</label><select name="lecaTemp">
					<option value="0" selected="selected">전송</option>
					<option value="1">임시저장</option>
				</select></p>	
			</div>
			
			<button type="submit"></button>
			
		<sec:csrfInput />
		</form>
	</div>
	

	
  <div class="item content2"></div>
  <div class="item content3"></div>
  <div class="item content4"></div>
  <div class="item content5"></div>
  <div class="item content6"></div>
  <div class="item content7"></div>
  <div class="item content8"></div>
</section>

<script>
  logout = function() {
    $("#logoutForm").submit();
  }
  
</script>
<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags"%>



<sec:authentication property="principal.userVO" var="vo"/>


<div class="sub_menu">
  <div class="sub_title">내강의실</div>
  
  <div class="submenu_menu">

	<sec:authorize access="isAuthenticated()">
		<sec:authentication property="principal.userVO" var="userVO" />
	</sec:authorize>
	<c:forEach items="${enrolmentVOList}" var="enrolmentVO" varStatus="stat">
	    <a class="sub_menu_a" href="/student/lecture/detail?lecCd=${enrolmentVO.lectureVO.lecCd}">
	    	${enrolmentVO.lectureVO.lectureApplyVO.lecaNm}
		</a>
	</c:forEach>
  </div>
</div>
<script>
  $.ajax({
    url: "/common/getLectureName?lecCd=${param.lecCd}",
    type: "get",
    contentType: "application/json; charset=UTF-8",
    async: false,
    success: res => {
      $(".sub_title").text(res);
    },
    error: xhr => {
      console.log(xhr);
    }
  })
</script>
.btn-two {margin: 9px; text-decoration: none;}
.btn-two.mini, .btn-gradient.mini, .btn-effect.mini {padding: 4px 12px;  font-size: 12px;}
.btn-two.block{display: block;width: 60%;margin-left: auto;margin-right: auto;text-align: center;}
/* Colors for .btn and .btn-two */
.btn.blue, .btn-two.blue     {background-color: #00a1b9;}
.btn.red, .btn-two.red       {background-color: #ff616b;}
.btn.gray, .btn-two.gray {background-color: #A3A3A3;}
.btn.green, .btn-two.green {background-color: #a6c76c;}
.btn.sky, .btn-two.sky {background-color: blue;}
.btn.orange, .btn-two.orange {background-color: #fa573c;}
.btn.dark, .btn-two.dark {background-color: #555555;}

.btn-two {
	color: white; border-radius:3px;
	padding: 15px 25px;
	display: inline-block;
	border: 1px solid rgba(0,0,0,0.21);
	border-bottom-color: rgba(0,0,0,0.34);
	text-shadow:0 1px 0 rgba(0,0,0,0.15);
	/* box-shadow: 0 1px 0 rgba(255,255,255,0.34) inset, 
				      0 2px 0 -1px rgba(0,0,0,0.13), 
				      0 3px 0 -1px rgba(0,0,0,0.08), 
				      0 3px 13px -1px rgba(0,0,0,0.21); */
}
.btn-two:active {
	top: 1px;
	border-color: rgba(0,0,0,0.34) rgba(0,0,0,0.21) rgba(0,0,0,0.21);
	box-shadow: 0 1px 0 rgba(255,255,255,0.89),0 1px rgba(0,0,0,0.05) inset;
	position: relative;
}


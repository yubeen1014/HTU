<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%>
<%@ taglib prefix="sec"
	uri="http://www.springframework.org/security/tags"%>
<style>
 .content{background-color:#ffffff;}
 .table_topic{font-size: 20px; padding-bottom:30px;}
 .prifile_wrap tr{display: table-row;}
 .profile_wrap th{width:170px; padding:20px; font-size:14px;}
 .profile_wrap td{padding:10px;}
 .profile_photo{width:100px!important; height:100px!important; border:1px solid #888888;}
</style>
<sec:authorize access="isAuthenticated()">
	<sec:authentication property="principal.userVO" var="userVO" />
</sec:authorize>

<p>${lectureVO.lectureApplyVO.roomVO}</p>

<div class="row" style="margin-bottom: 20px; margin-left: 1px;">
	<div class="col-lg-12">
		<h1 class="page-header">강의상세</h1>
	</div>
</div>
<div class="card mb-4">
      <h5 class="card-header"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">강의정보</font></font></h5>
      <div class="card-body">
        <div class="mb-3 row">
          <label for="html5-text-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">강의명</font></font></label>
          <div class="col-md-10">
           <span class="form-control">${lectureVO.lectureApplyVO.lecaNm}</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-text-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">교수명</font></font></label>
          <div class="col-md-10">
           <span class="form-control">${lectureVO.lectureApplyVO.professorVO.userVO.userNm}</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-email-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">개설여부</font></font></label>
          <div class="col-md-10">
            <c:if test="${lectureVO.lecStat == 0}"><span class="form-control">개강</span></c:if> 
            <c:if test="${lectureVO.lecStat == 1}"><span class="form-control">종강</span></c:if> 
            <c:if test="${lectureVO.lecStat == 2}"><span class="form-control">폐강</span></c:if> 
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-url-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">강의구분</font></font></label>
          <div class="col-md-10">
          	<c:if test="${lectureVO.lectureApplyVO.lecaType == 0}"><span class="form-control">전공필수</span></c:if> 
          	<c:if test="${lectureVO.lectureApplyVO.lecaType == 1}"><span class="form-control">전공선택</span></c:if> 
          	<c:if test="${lectureVO.lectureApplyVO.lecaType == 2}"><span class="form-control">교양필수</span></c:if> 
          	<c:if test="${lectureVO.lectureApplyVO.lecaType == 3}"><span class="form-control">교양선택</span></c:if> 
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-search-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">년도/학기</font></font></label>
          <div class="col-md-10">
            <span class="form-control">${lectureVO.lectureApplyVO.lecaYear}년&nbsp;${lectureVO.lectureApplyVO.lecaSem}학기</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-tel-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">학년</font></font></label>
          <div class="col-md-10">
             <span class="form-control">${lectureVO.lectureApplyVO.lecaTrg}학년</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-password-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">학과</font></font></label>
          <div class="col-md-10">
            <span class="form-control">${lectureVO.lectureApplyVO.departmentVO.depNm}</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-number-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">학점</font></font></label>
          <div class="col-md-10">
            <span class="form-control">${lectureVO.lectureApplyVO.lecaCrd}</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-datetime-local-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">요일</font></font></label>
          <div class="col-md-10">
            <span class="form-control">${lectureVO.lectureApplyVO.lecaSche}</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-date-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">시간</font></font></label>
          <div class="col-md-10">
            <span class="form-control">${lectureVO.lectureApplyVO.lecaStdt}&nbsp;~&nbsp;${lectureVO.lectureApplyVO.lecaEddt}</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-month-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">호실</font></font></label>
          <div class="col-md-10">
            <span class="form-control">${lectureVO.lectureApplyVO.roomCd}</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-week-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">교재&nbsp;및&nbsp;참고서</font></font></label>
          <div class="col-md-10">
            <span class="form-control">${lectureVO.lectureApplyVO.lecaBook}</span>
          </div>
        </div>
        <div class="mb-3 row">
          <label for="html5-time-input" class="col-md-2 col-form-label"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">강의개요</font></font></label>
          <div class="col-md-10">
            <span class="form-control">${lectureVO.lectureApplyVO.lecaCon}</span>
          </div>
        </div>
      </div>
    </div>



<script type="text/javascript">
logout = function() {
	$("#logoutForm").submit();
}
	
$(function(){
	$("#weekplanCreate").on("click", function(){
		console.log("왔다")
		
		var attributeValue = "${lectureVO.lecCd}"
		console.log("왔다", attributeValue);
		localStorage.setItem("lecCD", attributeValue);
		window.location.href = "/professor/weekplan/create?lecCd=" + ${lectureVO.lecCd};
	});
	$("#lectureNoticeCreate").on("click", function(){
		console.log("왔다")
		
		var attributeValue = "${lectureVO.lecCd}"
		console.log("왔다", attributeValue);
		localStorage.setItem("lecCD", attributeValue);
		window.location.href = "/professor/lecturenotice/create?lecCd=" + ${lectureVO.lecCd};
	});
	$("#lectureNoticeList").on("click", function(){
		console.log("왔다")
		
		var attributeValue = "${lectureVO.lecCd}"
		console.log("왔다", attributeValue);
		localStorage.setItem("lecCD", attributeValue);
		window.location.href = "/professor/lecturenotice/list?lecCd=" + ${lectureVO.lecCd};
	});
	
});	

</script>




.map{}

.floor{
  width: 1000px;
  height: 500px;
  
  box-sizing: border-box;
  position: relative; 
  width:90%; margin:20px auto; background-color:#ffffff!important; border-radius:20px; padding-bottom:50px;
}

.ho{
  width: 150px;
  height: 100px;
  border: 1px black solid;
  box-sizing: border-box;
  display: block;
  position: absolute;
  transition:.3s;
}

.ho:nth-child(1) {
  bottom: 0;
  left: 0;
}
.ho:nth-child(2) {
  bottom: 100px;
  left: 0;
}
.ho:nth-child(3) {
  bottom: 200px;
  left: 0;
}
.ho:nth-child(4) {
  top: 100px;
  left: 0;
}
.ho:nth-child(5) {
  top: 0;
  left: 0;
}
.ho:nth-child(6) {
  top: 0;
  left: 200px;
}
.ho:nth-child(7) {
  top: 0;
  left: 350px;
}
.ho:nth-child(8) {
  top: 0;
  left: 500px;
}
.ho:nth-child(9) {
  top: 0;
  left: 650px;
}
.ho:nth-child(10) {
  top: 0;
  left: 850px;
}
.ho:nth-child(11) {
  top: 100px;
  right: 0;
}
.ho:nth-child(12) {
  top: 200px;
  right: 0;
}
.ho:nth-child(13) {
  top: 300px;
  right: 0;
}
.ho:nth-child(14) {
  top: 400px;
  right: 0;
}
.ho:nth-child(15) {
  bottom: 0;
  right: 200px;
}
.ho:nth-child(16) {
  bottom: 0;
  right: 350px;
}
.ho:nth-child(17) {
  bottom: 0;
  right: 500px;
}
.ho:nth-child(18) {
  bottom: 0;
  right: 650px;
}
.ho span{display:block; font-size:12px; font-weight:600; color:#555555; margin:20px;}
.ho:hover{background-color:#555555; color:#ffffff;}
.sisul:hover{background-color:inherit;}
.sisul{border:none; padding:20px;}
.sisul img{margin:0 auto;}
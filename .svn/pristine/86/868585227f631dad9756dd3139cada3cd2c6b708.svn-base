	<update id="update" parameterType="userVO">
		UPDATE USERS
		SET USER_NM=#{userNm}
		, USER_NME=#{userNme}
		, USER_REG1=#{userReg1}
		, USER_REG2=#{userReg2}
		, USER_BANK=#{userBank}
		, USER_ACC=#{userAcc}
		, USER_ZIP=#{userZip}
		, USER_ADDR1=#{userAddr1}
		, USER_ADDR2=#{userAddr2}
		, USER_TEL=#{userTel}
		, USER_MAIL=#{userMail}
		WHERE USER_CD=#{userCd}
	</update>
	
